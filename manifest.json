{
    "name": "Fast.com Converter",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["converter.js"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["ytDropdown.js"]
        }
        
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "converter"
    }
}